{
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": [ "FARGATE"],
  "executionRoleArn": "$AWS_ECS_TASK_EXECUTION_ROLE_ARN",
  "containerDefinitions":
  [
    {
      "name": "migrate",
      "image": "$AWS_ECR_ACCOUNT_URL/sample-project-app-development:latest",
      "essential": true,
      "entryPoint": ["/bin/sh"],
      "command": ["-c", "php artisan migrate --force"],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/sample-project-dev-migrate",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "migrate"
        }
      },
      "environment": [
        {
          "name": "APP_NAME",
          "value": "sample-project"
        },
        {
          "name": "APP_ENV",
          "value": "development"
        },
        {
          "name": "APP_DEBUG",
          "value": "true"
        },
        {
          "name": "LOG_CHANNEL",
          "value": "cloudwatch"
        },
        {
          "name": "CLOUDWATCH_LOG_RETENTION",
          "value": "60"
        },
        {
          "name": "DB_CONNECTION",
          "value": "mysql"
        },
        {
          "name": "DB_HOST",
          "value": "$DB_HOST_DEV"
        },
        {
          "name": "DB_PORT",
          "value": "3306"
        },
        {
          "name": "DB_USERNAME",
          "value": "organizer"
        },
        {
          "name": "DB_DATABASE",
          "value": "event_organizer"
        },
        {
          "name": "BROADCAST_DRIVER",
          "value": "log"
        },
        {
          "name": "CACHE_DRIVER",
          "value": "redis"
        },
        {
          "name": "SESSION_DRIVER",
          "value": "redis"
        },
        {
          "name": "SESSION_LIFETIME",
          "value": "10080"
        },
        {
          "name": "REDIS_HOST",
          "value": "$REDIS_HOST_DEV"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "null"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        },
        {
          "name": "AWS_BUCKET",
          "value": "sample-project.dev.uploads"
        },
        {
          "name": "MAIL_DRIVER",
          "value": "ses"
        },
        {
          "name": "MAIL_FROM_ADDRESS",
          "value": "tmp"
        },
        {
          "name": "MAIL_FROM_NAME",
          "value": "dev"
        },
        {
          "name": "SES_REGION",
          "value": "us-east-1"
        },
        {
          "name": "ADMIN_HTTPS",
          "value": "true"
        }
      ]
    }
  ]
}